{
  "name": "simple-ts-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn install && ts-node-dev --ignore-watch ./node_modules --transpile-only --respawn -r dotenv/config ./src/server.ts",
    "build": "tsc -p .",
    "start": "yarn build && yarn install && ts-node-dev --ignore-watch ./node_modules --transpile-only --respawn -r dotenv/config ./src/server.ts",
    "prepare": "husky install",
    "format": "pretty-quick --staged"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^5.3.0",
    "firebase": "^8.9.0",
    "firebase-admin": "^9.11.0",
    "husky": "^7.0.1",
    "morgan": "^1.10.0",
    "prettier": "^2.3.2",
    "prettier-quick": "^0.0.5",
    "pretty-quick": "^3.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21"
  }
}
